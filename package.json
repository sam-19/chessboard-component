{
  "name": "@sam-19/chessboard-component",
  "version": "0.1.3",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./types": "./dist/types/index.d.ts",
    "./package.json": "./package.json",
    "./dist/types": "./dist/types/index.d.ts",
    "./umd/chessboard-component.js": "./umd/chessboard-component.js",
    "./umd/chessboard-component.umd.cjs": "./umd/chessboard-component.umd.cjs"
  },
  "files": [
    "dist/*",
    "umd/*",
    "umd/*.LICENSE.txt",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "dev": "vite",
    "build": "npm run build:package && npm run build:standalone",
    "build:manifest": "node ./node_modules/@custom-elements-manifest/analyzer/cem.js analyze",
    "build:package": "tsc && vite build --config vite.config.package.ts",
    "build:standalone": "tsc && vite build --config vite.config.standalone.ts",
    "prepack": "npm run build",
    "preview": "vite preview",
    "release": "npm publish --access=public"
  },
  "dependencies": {
    "lit": "^3.2.0"
  },
  "devDependencies": {
    "@custom-elements-manifest/analyzer": "^0.10.3",
    "@types/node": "^22.7.7",
    "typescript": "^5.5.3",
    "vite": "^5.4.8",
    "vite-plugin-singlefile": "^2.0.2",
    "vite-tsconfig-paths": "^5.0.1"
  }
}
